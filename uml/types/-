@startuml
 folder "Publishers" {
  [pub 1]
  [pub2]
 }

 node "Simulation" {
  [Internal Messages]
 }

 node "Implementation" {
  [Internal Messages]
  [External Messages]
 }

 node "Communication Method" {
  [Radio]
  [TCP/UDP]
 }

 database "Publisher Data" {
  [pub 1 data]
  [pub 2 data]
 }



 [Simulation] --> [Publishers] : Inititialization 
 [Implementation] --> [Publishers] : Inititialization 
 [Simulation] <-- [Publishers] : Publish Data 
 [Implementation] <-- [Publishers] : Publish Data 
 
@enduml
